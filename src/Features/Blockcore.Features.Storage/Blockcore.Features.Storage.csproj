    </Reference>
    <Reference Include="MessagePack.Annotations">
      <HintPath>..\..\Libs\MessagePack\netcoreapp2.1\MessagePack.Annotations.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
